pipeline {
    agent any
	tools {
	    maven "3.6.3"
	 	}
		stages {
	           stage('Git CheckOut') {
		        steps {
			   git branch: 'develop', credentialsId: '0f56fcea-4708-41e4-80d3-e602b12d22a4', url: 'https://github.com/trakeshkumar34/jenkins_practice.git'
			}
			stage('Clean and Install') {
               steps {
                bat 'mvn clean install'
            }
        }
		}
       }
	  } 