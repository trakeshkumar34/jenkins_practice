pipeline {
    agent any
	
		stages {
	           stage('Git CheckOut') {
		        steps {
			   git branch: 'master', credentialsId: '0f56fcea-4708-41e4-80d3-e602b12d22a4', url: 'https://github.com/trakeshkumar34/maven-project.git'
			}
			}
			stage('Clean and Install') {
               steps {
                bat 'mvn clean install'
            }
        
		}
       }
	  } 
